// Encrypt data here: https://app.renovatebot.com/encrypt
{
  "hostRules": [
    {
      "hostType": "docker",
      "matchHost": "docker.io",
      "encrypted": {
        "username": "{{ secrets.DOCKERHUB_USER }}",
        "password": "{{ secrets.DOCKERHUB_TOKEN }}"
      }
    },
    {
      "hostType": "docker",
      "matchHost": "ghcr.io",
      "encrypted": {
        "username": "{{ secrets.GHCR_USER }}",
        "password": "{{ secrets.GHCR_TOKEN }}"
      }
    }
  ]
}
